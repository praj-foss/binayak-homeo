<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="400.0" minWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.172" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.binayak.ui.controllers.SavedCasesController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <HBox alignment="CENTER" maxHeight="26.0" minHeight="26.0" prefHeight="26.0">
         <children>
            <Label text="SAVED CASES">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="btnOpen" mnemonicParsing="false" prefWidth="80.0" text="Open" />
         </children>
      </HBox>
      <TableView fx:id="tableCases" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="tableColDate" prefWidth="75.0" text="Date" />
          <TableColumn fx:id="tableColName" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="tableColAge" prefWidth="75.0" text="Age" />
            <TableColumn fx:id="tableColSex" prefWidth="75.0" text="Sex" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
